{
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "javascript.format.enable": false,
    "editor.formatOnSave": true,
    "minapp-vscode.prettier": {
        "useTabs": false,
        "tabWidth": 4,
        "printWidth": 100,
        "singleQuote": false
    },
    "prettier.printWidth": 140,
    "prettier.semi": false,
    "prettier.tabWidth": 4,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "javascript.preferences.quoteStyle": "single",
    "typescript.preferences.quoteStyle": "single",
    "prettier.jsxSingleQuote": true,
    "prettier.singleQuote": true,
    "eslint.format.enable": true,
    "eslint.probe": ["javascript", "javascriptreact", "html", "vue", "markdown"],
    "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
    "typescript.format.enable": true,
    "typescript.reportStyleChecksAsWarnings": true,
    "typescript.suggestionActions.enabled": true,
    "typescript.surveys.enabled": true,
    "typescript.tsc.autoDetect": "on",
    "typescript.validate.enable": true,
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#d9ab00",
        "activityBar.activeBorder": "#dffff8",
        "activityBar.background": "#d9ab00",
        "activityBar.foreground": "#15202b",
        "activityBar.inactiveForeground": "#15202b99",
        "activityBarBadge.background": "#dffff8",
        "activityBarBadge.foreground": "#15202b",
        "statusBar.background": "#a68300",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#d9ab00",
        "titleBar.activeBackground": "#a68300",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#a6830099",
        "titleBar.inactiveForeground": "#e7e7e799",
        "sash.hoverBorder": "#d9ab00",
        "statusBarItem.remoteBackground": "#a68300",
        "statusBarItem.remoteForeground": "#e7e7e7"
    },
    "peacock.color": "#A68300",
    "git.ignoreLimitWarning": true,
    "cSpell.words": [
        "baiheliangchi",
        "openid",
        "unionid",
        "UNIONID",
        "cloudbase",
        "jingangdaodui",
        "lanzhayi",
        "liufengsibi",
        "danbian",
        "xiexing",
        "zhongpan",
        "shantongbei",
        "tarojs",
        "subpages"
    ]
}
